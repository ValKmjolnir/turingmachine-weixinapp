<!--pages/files/files.wxml-->

<view class="page-body">
    <view class="button-container" style="text-align: center;font-weight: 700;margin-top: 20rpx;">
        <view wx:if="{{empty_file_list}}" style="font-size: 27rpx;font-weight: 400;color: #5a6877;">
            啊哦，什么都没有...
        </view>
        <view wx:for="{{files}}">
            <button bindtap="gotoedit" data-param="{{item}}" style="color: #5a6877;">
                {{item}}
            </button>
        </view>
    </view>
</view>
